{
  "family": "supabase-gotrue",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::307621978585:role/supabase-task-execution-role",
  "taskRoleArn": "arn:aws:iam::307621978585:role/supabase-task-role",
  "containerDefinitions": [
    {
      "name": "gotrue",
      "image": "supabase/gotrue:v2.46.3",
      "portMappings": [
        {
          "containerPort": 9999,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "GOTRUE_DB_DRIVER",
          "value": "postgres"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgresql://supabase:d5-Zm>zWS*OyGVqsbHPt[zul9V$g@supabase-aurora-cluster.cluster-c9memqg6633m.us-west-2.rds.amazonaws.com:5432/supabase"
        },
        {
          "name": "API_EXTERNAL_URL",
          "value": "http://supabase-studio-alb-1499081913.us-west-2.elb.amazonaws.com"
        },
        {
          "name": "GOTRUE_SITE_URL",
          "value": "https://d2bxqn3xh4v4kj.cloudfront.net"
        },
        {
          "name": "GOTRUE_API_HOST",
          "value": "0.0.0.0"
        },
        {
          "name": "GOTRUE_API_PORT",
          "value": "9999"
        },
        {
          "name": "GOTRUE_JWT_SECRET",
          "value": "your-super-secret-jwt-token-with-at-least-32-characters-long"
        },
        {
          "name": "GOTRUE_JWT_DEFAULT_GROUP_NAME",
          "value": "authenticated"
        },
        {
          "name": "GOTRUE_JWT_AUD",
          "value": "authenticated"
        },
        {
          "name": "GOTRUE_JWT_ADMIN_ROLES",
          "value": "service_role"
        },
        {
          "name": "GOTRUE_EXTERNAL_EMAIL_ENABLED",
          "value": "true"
        },
        {
          "name": "GOTRUE_DISABLE_SIGNUP",
          "value": "false"
        },
        {
          "name": "GOTRUE_URI_ALLOW_LIST",
          "value": "*"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/supabase-gotrue",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "gotrue"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -sf http://localhost:9999/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 30
      }
    }
  ]
}
