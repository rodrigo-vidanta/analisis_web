{
  "family": "pgmeta-with-n8n-db",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::307621978585:role/supabase-task-execution-role",
  "taskRoleArn": "arn:aws:iam::307621978585:role/supabase-task-role",
  "containerDefinitions": [
    {
      "name": "postgres-meta-n8n-db",
      "image": "supabase/postgres-meta:v0.93.1",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PG_META_PORT",
          "value": "8080"
        },
        {
          "name": "PG_META_DB_HOST",
          "value": "n8n-postgres.c9memqg6633m.us-west-2.rds.amazonaws.com"
        },
        {
          "name": "PG_META_DB_PORT",
          "value": "5432"
        },
        {
          "name": "PG_META_DB_NAME",
          "value": "postgres"
        },
        {
          "name": "PG_META_DB_USER",
          "value": "n8nuser"
        },
        {
          "name": "PG_META_DB_PASSWORD",
          "value": "N8n_Secure_2024!"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pgmeta-with-n8n-db",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
