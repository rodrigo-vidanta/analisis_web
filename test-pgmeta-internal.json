{
  "family": "test-pgmeta-internal",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::307621978585:role/supabase-task-execution-role",
  "containerDefinitions": [
    {
      "name": "curl-test",
      "image": "curlimages/curl:latest",
      "command": ["sh", "-c", "curl -v http://10.0.101.100:8080/tables?include_columns=false&included_schemas=public && sleep 10"],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/test-pgmeta-internal",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
